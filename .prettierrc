{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "tabWidth": 2,
  "printWidth": 100,
  "jsxSingleQuote": false,
  "plugins": [
    "prettier-plugin-classnames",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-merge"
  ],
  "tailwindFunctions": ["clsx", "cn", "classnames"]
}
